{
  "websocket": {
    "port": 8000,
    "hostname": "0.0.0.0"
  },
  "brokers":{
    "amqp": [{
      "name": "rabbitmq1",
      "host": "127.0.0.1",
      "port": "5712",
      "user": "guest",
      "password": "guest",
      "vHost": "/"
    },{
      "name": "rabbitmq2",
      "host": "127.0.0.2",
      "port": "5712",
      "user": "guest",
      "password": "guest",
      "vHost": "/"
    }],
    "kafka": [{
      "name": "kafka1",
      "hosts": ["127.0.0.1:9092"]
    },{
      "name": "kafka2",
      "hosts": ["127.0.0.2:9092"]
    }]
  },
  "dispatcher": [
    {
      "field": "messageType",
      "value": "createUser",
      "route": {
        "broker": "rabbitmq1",
        "queue": "createUser"
      }
    },
    {
      "field": "messageType",
      "value": "createUser",
      "route": {
        "broker": "kafka1",
        "queue": "createUser"
      }
    },
    {
      "field": "messageType",
      "value": "updateUser",
      "route": {
        "broker": "rabbitmq1",
        "queue": "updateUser"
      }
    }
  ]
}